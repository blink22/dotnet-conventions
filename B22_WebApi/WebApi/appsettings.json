    // Example: Server=Blink22\\MSSQLSERVER01
  },
  "ExampleVarGroup1Name": {
    "VarKey": "VarValue"
    // Example variable group 1 with a key-value pair
  },
  "ExampleVarGroup2Name": {
    "BLINK22-Domain": "www.blink22.com"
    // Example variable group 2 with a key-value pair
  }
}
