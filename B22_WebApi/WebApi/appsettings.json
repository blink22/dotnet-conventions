{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*", // Allow requests from any host
  "ConnectionStrings": {
    "DefaultConnection": "Server={Local SQL Server Name};Database=Blink22DotNet;Trusted_Connection=True;TrustServerCertificate=True;"
